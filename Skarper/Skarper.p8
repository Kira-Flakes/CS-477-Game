function _init()
  init_splash()
end

function init_splash()
  skunk={
    sp=204,
    head1_sp=235,
    head2_sp=251,
    anim1=0,
    anim2=0,
    x=50,
    y=32,
    head1x=50,
    head2x=58,
    head1y=56,
    head2y=56
  }

  _update = update_splash
  _draw = draw_splash
end

function init_menu()
  _update = update_menu
  _draw = draw_menu
end

function init_game()
  player={
    sp=0,
    x=115,
    y=296,
    w=16,
    h=16,
    flp=false,
    dx=0,
    dy=0,
    max_dx=2.5,
    max_dy=3,
    acc=0.5,
    dash_time=0,
    dash_target=10,
    dash_acc=2,
    boost=4.4, --jump value
    anim=0,
    running=false,
    jumping=false,
    falling=false,
    sliding=false,
    landed=false,
    dashing=false
  }
  
  gravity=0.4
  friction=0.85
  
  --simple camera
  cam_x=0
  cam_y=0
  
  --map limits
  map_start=0
  map_end_x=384
  map_end_y=320

  --set state
  _update = update_game
  _draw = draw_game
end --end init_game

function init_win()
  _update = update_win
  _draw = draw_win
end

function init_lose()
  _update = update_lose
  _draw = draw_lose
end

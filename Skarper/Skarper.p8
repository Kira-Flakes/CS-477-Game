--player functions--

function player_update()
  --physics
  player.dy+=gravity
  player.dx*=friction
     
      --run
		    if btn(⬅️) then
		      player.dx-=player.acc
		      player.running=true
		      player.flp=true
		    end
		  
		    if btn(➡️) then
		      player.dx+=player.acc
		      player.running=true
		      player.flp=false
		    end
		  
		    --jump
		    if btnp(⬆️)
		    and player.landed then
		      player.dy-=player.boost
		      player.jumping=true
		      player.landed=false
		    end
		  
		    --stop running
		    if player.running
		    and not btn(⬅️)
		    and not btn(➡️)
		    and not player.falling
		    and not player.jumping then
		      if abs(player.dx)<.2 then
		        player.running=false
		        player.dx=0
		      end
		    end
  
  
  --check collision up and down
  
  if player.dy>0 then
    player.falling=true
    player.landed=false
    player.jumping=false
    
    player.dy=limit_speed(player.dy,player.max_dy)
    --if down pressed, don't check collision
    if not btn(⬇️) then
      --check down collision for platforms
      if collide_map(player,"down",1) then
        player.landed=true
        player.falling=false
        player.dy=0
        player.y-=((player.y+player.h+1)%8)-1
    
      end
    end
    --check down collision for map
    if collide_map(player,"down",0) then
        player.landed=true
        player.falling=false
        player.dy=0
        player.y-=((player.y+player.h+1)%8)-1
    
    end
  end
  
  if player.dy<0 then
    player.jumping=true
    if collide_map(player,"up",0) then
      player.dy=0
      
    end
  end
  
  --check collision left and right
  if player.dx<0 then
  
    player.dx=limit_speed(player.dx,player.max_dx)
  
    if collide_map(player,"left",0) then
      player.dx=0
    end
    
    --check collision with exit
    if collide_map(player,"left",7) then
      --switch to win screen
      player.x=0
      player.y=0
      init_win()
    end 
  elseif player.dx>0 then
  
    player.dx=limit_speed(player.dx,player.max_dx)
  
    if collide_map(player,"right",0) then
      player.dx=0
    end
    
    --check collision with exit
    if collide_map(player,"right",7) then
      --switch to win screen
      player.x=0
      player.y=0
      init_win()
    end  
  end
  
  player.x+=player.dx
  player.y+=player.dy

  --limit player to map
  if player.x<map_start then
    player.x=map_start
  end
  if player.x>map_end_x-player.w then
    player.x=map_end_x-player.w
  end
  
  if player.y<map_start then
    player.y=map_start
  end
  if player.y>map_end_y-player.h then
    player.y=map_end_y-player.h
  end
end --end player_update



function player_animate()
  if player.jumping then
    if player.sp != 12 then
      player.sp = 10
      if time() - player.anim>.2 then
        player.sp += 2
        player.anim=time()
      end
    end
  elseif player.falling then
    player.sp = 14
  elseif player.running then
    if time() - player.anim>.16 then
      player.sp += 2
      player.anim=time()
      
      if player.sp > 8 then
        player.sp = 2
      end
    end
  else --player idle
    player.sp=0
  end
end --end player_animate


function limit_speed(num,maximum)
  return mid(-maximum,num,maximum)
end --end limit_speed
